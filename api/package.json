{
  "name": "url-shortner",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.ts",
  "description": "",
  "scripts": {
    "dev:tsc": "tsc -b --watch --preserveWatchOutput",
    "dev:node": "node --env-file=.env --enable-source-maps --watch dist/src/index.js",
    "dev": "pnpm build && pnpm run \"/dev:/\"",
    "build": "tsc -b",
    "prestart": "pnpm build",
    "start": "node --env-file=.env dist/src/index.js",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "db:introspect": "drizzle-kit introspect --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "tsx --env-file=.env src/db/migrate.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts"
  },
  "keywords": [],
  "author": "Omkar Date",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^0.9.4",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.32.0",
    "express": "^4.19.2",
    "nanoid": "^5.0.7",
    "whatwg-url": "^14.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/whatwg-url": "^11.0.5",
    "@types/ws": "^8.5.11",
    "drizzle-kit": "^0.23.0",
    "prettier": "^3.3.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
